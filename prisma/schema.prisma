datasource db {
  provider = "sqlite"
  url      = "file:./rooms.db"
}

generator client {
  provider = "prisma-client-js"
}

model Event {
  id        String   @id @default(cuid())
  type      String
  title     String
  hour      String
  classroom String
  duration  String
  note      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
