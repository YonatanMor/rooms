    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint"
    ],
    "*.js": "eslint --cache --fix",
    "**/*.+(js|jsx|ts|tsx)": [
      "prettier --write",
      "git add"
    ]
  }
}
