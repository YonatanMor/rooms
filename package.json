{
  "name": "songi-8c30",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "node ./server.mjs",
    "build": "remix vite:build",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "pretest:e2e:run": "npm run build",
    "tc": "tsc -b",
    "validate": "run-p lint tc \"test -- --run\"",
    "db:setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "db:migrate": "prisma migrate dev --name ",
    "db:generate": "prisma generate",
    "db:proxy": "flyctl proxy 5433 -a songi-db",
    "db:seed": "prisma db seed",
    "db:seed-from-prisma": "ts-node --require tsconfig-paths/register prisma/seed.ts",
    "prepare": "husky install"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@prisma/client": "^4.3.1",
    "@radix-ui/themes": "^3.1.1",
    "@remix-run/express": "^2.9.1",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "@remix-run/server-runtime": "^2.9.1",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "framer-motion": "^7.6.5",
    "isbot": "^3.5.3",
    "jsdom": "^20.0.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "tailwind-merge": "^2.3.0",
    "tslib": "^2.4.1",
    "type-fest": "^3.12.0",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.1",
    "@remix-run/eslint-config": "^2.9.1",
    "@remix-run/testing": "^2.9.1",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.14",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.11",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cssnano": "^5.1.14",
    "dotenv": "^16.0.2",
    "esbuild": "^0.15.8",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^9.1.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "msw": "^0.47.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prisma": "^4.3.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.4.5",
    "ts-node": "^10.9.1",
